                            <xs:appinfo><meta:version added-in="5.5"/></xs:appinfo>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Expected SSL thumbprint of the endpoint.
                            </xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                         </xs:annotation>
                     </xs:element>
                     <xs:element name="LocalCopy" type="xs:boolean" minOccurs="0" maxOccurs="1">
                         <xs:annotation>
                             <xs:appinfo><meta:version added-in="5.5"/></xs:appinfo>
                             <xs:documentation source="modifiable">always</xs:documentation>
                             <xs:documentation xml:lang="en">
                                  If true, synchronization creates a local copy
                                  of all items. Defaults to false, which
                                  defers creation of the local copy until the
                                  item is required by an operation such as
                                  instantiation.
                              </xs:documentation>
                              <xs:documentation source="required">false</xs:documentation>
                          </xs:annotation>
                      </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="PublishExternalCatalogParams" type="PublishExternalCatalogParamsType"/>
    <xs:complexType name="PublishExternalCatalogParamsType">
        <xs:annotation>
            <xs:appinfo><meta:version added-in="5.5"/></xs:appinfo>
            <xs:appinfo>
                <meta:content-type>application/vnd.vmware.admin.publishExternalCatalogParams</meta:content-type>
            </xs:appinfo>
            <xs:documentation source="since">5.5</xs:documentation>
            <xs:documentation xml:lang="en">
                 Configuration parameters for a catalog that is published
                 externally.
            </xs:documentation>
        </xs:annotation>

        <xs:complexContent>
            <xs:extension base="VCloudExtensibleType">
                <xs:sequence>
                    <xs:element name="IsPublishedExternally" type="xs:boolean" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:appinfo><meta:version added-in="5.5"/></xs:appinfo>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                True enables external publication as configured
                                by these parameters.
                            </xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="catalogPublishedUrl" type="xs:string" minOccurs="0">
                        <xs:annotation>
                            <xs:appinfo><meta:version added-in="5.5"/></xs:appinfo>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Read-only endpoint URL supplied by the server.
                                External subscribers can connect to the catalog
                                at this URL.
                            </xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                         </xs:annotation>
                     </xs:element>
                     <xs:element name="Password" type="xs:string" minOccurs="0">
                         <xs:annotation>
                             <xs:appinfo><meta:version added-in="5.5"/></xs:appinfo>
                             <xs:documentation source="modifiable">always</xs:documentation>
                             <xs:documentation xml:lang="en">
                                 Password required when connecting to the
                                 endpoint.
                             </xs:documentation>
                             <xs:documentation source="required">false</xs:documentation>
                         </xs:annotation>
                     </xs:element>
                    <xs:element name="IsCacheEnabled" type="xs:boolean" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:appinfo><meta:version added-in="5.5"/></xs:appinfo>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                True enables content caching for this catalog.
                                All items in the catalog are created and stored
                                in transfer storage. If false, items are not
                                placed in transfer storage until they are
                                requested by a subscriber.
                            </xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="PreserveIdentityInfoFlag" type="xs:boolean" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:appinfo><meta:version added-in="5.5"/></xs:appinfo>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                True includes BIOS UUIDs and MAC addresses in the
                                downloaded OVF package. If false, those information
                                will be excluded.
                            </xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

